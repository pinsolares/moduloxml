<?xml version="1.0"? encoding="UTF-8"?>
<!DOCTYPE ps:instituto [
<!ELEMENT ps:instituto (ps:pas, pro:profesor, alu:alumno)>
    <!ATTLIST ps:instituto xmlns:ps CDATA #FIXED "http://es.instituto.org/ins/espacios_de_nombres/pas">
    <!ATTLIST ps:instituto xmlns:pro CDATA #FIXED "http://es.instituto.org/ins/espacios_de_nombres/pro">
    <!ATTLIST ps:instituto xmlns:alu CDATA #FIXED "http://es.instituto.org/ins/espacios_de_nombres/alu">

    <!ATTLIST ps:instituto especialidad CDATA #IMPLIED>

  <!ELEMENT ps:pas (ps:nombre, ps:apellidos, ps:puesto, ps:tlf)>
    <!ELEMENT ps:nombre (#PCDATA)>
    <!ELEMENT ps:apellido (#PCDATA)>
    <!ELEMENT ps:puesto (#PCDATA)>
    <!ELEMENT ps:tlf (#PCDATA)>

