    <!ELEMENT pro:nombre (#PCDATA)>
    <!ELEMENT pro:apellido (#PCDATA)>
    <!ELEMENT pro:asignatura (#PCDATA)>
    <!ELEMENT pro:tlf (#PCDATA)>

  <!ELEMENT alu:alumno (alu:nombre, alu:apellidos, alu:curso, alu:tlf)>
    <!ELEMENT alu:nombre (#PCDATA)>
    <!ELEMENT alu:apellido (#PCDATA)>
    <!ELEMENT alu:curso (#PCDATA)>
    <!ELEMENT alu:tlf (#PCDATA)>
]>
<ps:instituto especialidad="">
    <ps:pas>
        <pas:nombre></pas:nombre>
        <pas:apellidos></pas:apellidos>
        <pas:puesto></pas:puesto>
        <pas:tlf></pas:tlf>
    </ps:pas>
    
    <pro:profesor>
        <pro:nombre></pro:nombre>
        <pro:apellidos></pro:apellidos>
        <pro:asignatura></pro:asignatura>
        <pro:tlf></pro:tlf>
    </pro:profesor>
    
    <alu:alumno>
        <alu:nombre></alu:nombre>
        <alu:apellidos></alu:apellidos>
        <alu:curso></alu:curso>
        <alu:tlf></alu:tlf>
    </alu:alumno>
</ps:instituto>